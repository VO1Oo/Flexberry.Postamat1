<h3 class="ui header">{{t "forms.i-i-s-postamat-сген-код-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.сгенКод
      class=(if (v-get validationModel "сгенКод" "isInvalid") "error")
      label=(t "forms.i-i-s-postamat-сген-код-e.сгенКод-caption")
      data-test-i-i-s-postamat-сген-код-e-сгенКод=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "сгенКод" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.пользователь.фИО
      class=(if (v-get validationModel.пользователь "фИО" "isInvalid") "error")
      label=(t "forms.i-i-s-postamat-сген-код-e.пользователь.фИО-caption")
      data-test-i-i-s-postamat-сген-код-e-пользователь-фИО=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.пользователь "фИО" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.пользователь.номерТелефона
      class=(if (v-get validationModel.пользователь "номерТелефона" "isInvalid") "error")
      label=(t "forms.i-i-s-postamat-сген-код-e.пользователь.номерТелефона-caption")
      data-test-i-i-s-postamat-сген-код-e-пользователь-номерТелефона=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.пользователь "номерТелефона" "message")}}
  </div>
</form>
